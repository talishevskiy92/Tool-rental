	: TForm(Owner)
{
}
//---------------------------------------------------------------------------
void __fastcall TForm5::FormActivate(TObject *Sender)
{
DM->ADOQ->Close();
DM->ADOQ->SQL->Clear();
DM->ADOQ->SQL->Add("");
DM->ADOQ->Open();
}
